<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="TelescopeLayout">
    <attr name="pointerCount" format="integer"/>
    <attr name="progressColor" format="color"/>
    <attr name="screenshotMode" format="enum">
      <enum name="system" value="0"/>
      <enum name="canvas" value="1"/>
      <enum name="none" value="2"/>
    </attr>
    <attr name="screenshotChildrenOnly" format="boolean"/>
    <attr name="vibrate" format="boolean"/>
  </declare-styleable>
</resources>
